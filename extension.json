{
	"name": "Cloudflare",
	"version": "0.2.0",
	"author": [
		"harugon"
	],
	"url": "https://github.com/harugon/mediawiki-extensions-cloudflare",
	"descriptionmsg": "cloudflare-desc",
	"license-name": "MIT",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"MessagesDirs": {
		"Cloudflare": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Cloudflare\\": "includes/"
	},
	"ConfigRegistry": {
		"cloudflare": "GlobalVarConfig::newInstance"
	},
	"config": {
		"CloudflareEmail": {
			"description": "Cloudflare Email",
			"public": false,
			"value": false
		},
		"CloudflareAPIKey": {
			"description": "Cloudflare APIKey",
			"public": false,
			"value": false
		},
		"CloudflareZoneID": {
			"description": "Cloudflare Zon ID",
			"public": false,
			"value": false
		},
		"CloudflarePurgePage": {
			"description": "Purge a page from cache",
			"public": true,
			"value": false
		},
		"CloudflarePurgeFile": {
			"description": "Purge a File from cache",
			"public": true,
			"value": true
		}
	},
	"ExtensionFunctions": [
		"MediaWiki\\Extension\\Cloudflare\\Cloudflare::onExtensionFunction"
	],
	"ServiceWiringFiles": [
		"includes/ServiceWiring.php"
	],
	"manifest_version": 2
}
